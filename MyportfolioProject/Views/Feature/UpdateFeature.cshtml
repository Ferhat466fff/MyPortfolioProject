@model MyportfolioProject.Models.Tbl_Features
@{
    ViewBag.Title = "UpdateFeature";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3> Öne Çıkanlar Güncelleme Sayfası</h3>

<form method="post">
    <input type="hidden" name="FatureId" value="@Model.FatureId" />
    <label>İsim Soyisim</label>
    <input type="text" name=NameSurname value="@Model.NameSurname"class="form-control" />
    <br />
    <label>Ünvan</label>
    <input type="text" name=Title value="@Model.Title"class="form-control" />
    <br />
    <label>Resim URL</label>
    <input type="text" name=ImageUrl value="@Model.ImageUrl"class="form-control" />
    <br />

    <button class="btn btn-outline-dark">Güncelle</button>

</form>

