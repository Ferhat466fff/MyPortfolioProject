@model MyportfolioProject.Models.Tbl_Testimonials
@{
    ViewBag.Title = "AddTestimonial";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>Refarans Ekleme Sayfası</h3>

<form method="post">
    <label>Resim Adresi</label>
    <input type="text" name=ImageUrl class="form-control" />
    <br />
    <label>Yorum</label>
    <input type="text" name=Comment class="form-control" />
    <br />
    <label>İsim Soyisim</label>
    <input type="text" name=NameSurname class="form-control" />
    <br />

    <label>Ünvan</label>
    <input type="text" name=Title class="form-control" />
    <br />
    <label>Durum</label>
    <input type="text" name=Status class="form-control" />
    <br />
    <label>Yorum Tarihi</label>
    <input type="text" name=CommentDate class="form-control" />
    <br />

    <button class="btn btn-outline-dark">Kaydet</button>

</form>

