@model MyportfolioProject.Models.Tbl_Contacts
@{
    ViewBag.Title = "UpdateContact";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yeni İletişim Bilgileri Ekleme Sayfası</h2>
<br />
<br />

<form method="post">
    <input type="hidden" name="ContactId" value="@Model.ContactId"  />

    <label>İsim Soyisim</label>
    <input type="text" name="NameSurname" value="@Model.NameSurname" class="form-control" />
    <br />
    <label>Adres</label>
    <input type="text" name="Address" value="@Model.Address"class="form-control" />
    <br />
    <label>Telefon</label>
    <input type="text" name="Phone"value="@Model.Phone" class="form-control" />
    <br />
    <label>Email</label>
    <input type="text" name="Email" value="@Model.Email"class="form-control" />
    <br />


    <button class="btn btn-outline-primary">Kaydet</button>





</form>



