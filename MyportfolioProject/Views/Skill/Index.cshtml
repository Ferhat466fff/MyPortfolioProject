@model List<MyportfolioProject.Models.Tbl_Skills>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<br />
<h3>Yetenek Sayfası</h3>

<table class="table table-bordered">

    <tr>
        <th>#</th>
        <th>Yetenek Adı</th>
        <th>Yetenek Bilgisi</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.SkillId</td>
            <td>@item.SkillName</td>
            <td>@item.Value</td>
            <td><a href="/Skill/DeleteSkill/@item.SkillId" class="btn btn-outline-danger">Sil</a></td>
            <td><a href="/Skill/UpdateSkill/@item.SkillId" class="btn btn-outline-info">Güncelle</a></td>
        </tr>
    }
</table>
<td><a href="/Skill/AddSkill/" class="btn btn-outline-dark">Yeni İletişim Bilgileri Ekle</a></td>
